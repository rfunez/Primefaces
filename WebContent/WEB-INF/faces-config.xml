<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    <application>
		<resource-bundle>
			<base-name>pf.messages.messages</base-name>
			<var>msg</var>
		</resource-bundle>
		<locale-config>
			<supported-locale>pt_BR</supported-locale>
			<supported-locale>en_US</supported-locale>
		</locale-config>
	</application>
	<converter>
		<converter-id>pfConv</converter-id>
		<converter-class>pf.beans.PfConverter</converter-class>		
	</converter>
	<managed-bean>
		<managed-bean-name>comp1</managed-bean-name>
		<managed-bean-class>pf.beans.ComplementBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>idComp</property-name>
			<value>1</value>
		</managed-property>
		<managed-property>
			<property-name>name</property-name>
			<value>Triskys</value>
		</managed-property>
		<managed-property>
			<property-name>price</property-name>
			<value>22.00</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>comp2</managed-bean-name>
		<managed-bean-class>pf.beans.ComplementBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>idComp</property-name>
			<value>1</value>
		</managed-property>
		<managed-property>
			<property-name>name</property-name>
			<value>Gusanitos</value>
		</managed-property>
		<managed-property>
			<property-name>price</property-name>
			<value>18.00</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>listComp</managed-bean-name>
		<managed-bean-class>java.util.ArrayList</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<list-entries>
			<value-class>pf.beans.ComplementBean</value-class>
			<value>#{comp1}</value>
			<value>#{comp2}</value>
		</list-entries>
	</managed-bean>
    <managed-bean>
    	<managed-bean-name>pfBean</managed-bean-name>
    	<managed-bean-class>pf.beans.PrimefacesBean</managed-bean-class>
    	<managed-bean-scope>request</managed-bean-scope>
    </managed-bean>   
    <navigation-rule>
        <from-view-id>/pages/inicio.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>submit</from-outcome>
    		<to-view-id>/WEB-INF/jsp/index.jsp</to-view-id>
    	</navigation-case>
    	<navigation-case>
    		<from-outcome>close</from-outcome>
    		<to-view-id>/WEB-INF/jsp/close.jsp</to-view-id>
    	</navigation-case>
    </navigation-rule> 
    <navigation-rule></navigation-rule>
</faces-config>
